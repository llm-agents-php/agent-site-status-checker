{
  "name": "llm-agents/agent-site-status-checker",
  "description": "Simple agent to check the status of a website",
  "license": "MIT",
  "require": {
    "php": "^8.3",
    "llm-agents/agents": "^1.0",
    "spiral/boot": "^3.13"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.3"
  },
  "autoload": {
    "psr-4": {
      "LLM\\Agents\\Agent\\SiteStatusChecker\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LLM\\Agents\\Agent\\SiteStatusChecker\\Tests\\": "tests/src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
